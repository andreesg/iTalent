<div>
  <h2 style="text-decoration: underline;"> Eventos criados por mim: </h2>
  <% current_user.created_events.each do |event| %>
  <div class="well">
    <div>
      <p>t√≠tulo: <%= event.title %></p>
      <p>data: <%= event.date_start %></p>
    </div>
    <div class="btn-group">
      <%= link_to "eliminar", user_event_path(event), class: "btn btn-primary", method: :delete, remote: true %>
      <%= link_to "editar", edit_user_event_path(event), class: "btn btn-primary", remote: true %>
    </div>
  </div>
  </br>
  <% end %>
  <h2 style="text-decoration: underline;"> Eventos em que vou participar: </h2>
  <% current_user.event_attendees.each do |event_attendee| %>
  <div class="well">
    <%= render partial: 'timeline/event', locals: { event: event_attendee.event } %>
  </div>
  <% end %>
  </br>
</div>