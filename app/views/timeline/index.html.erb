<h1>Welcome to the timeline!</h1>

<h2>Publications</h2>
<ol>
<% @publications.each do |publication| %>
	<li><%=publication.text%></li>	
<% end %>
</ol>
<%= will_paginate @publications, param_name: 'publications_page' %>
<h2>Events</h2>
<ol>
<% @events.each do |event| %>
	<li><%=event.title%></li>
<% end %>	
</ol>
<%= will_paginate @events, param_name: 'events_page'  %>

